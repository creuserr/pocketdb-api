{
  "version": 2,
  "cleanUrls": true,
  "builds": [{
    "src": "/api/*.js",
    "use": "@vercel/node"
  }],
  "headers": [{
    "source": "/api/set.js",
    "headers": [{
      "key": "Access-Control-Allow-Origin",
      "value": "*"
    }, {
      "key": "Access-Control-Allow-Methods",
      "value": "GET,POST"
    }]
  }]
}